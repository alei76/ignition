<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

	<changeSet id="sample" author="jkolean">
			<createTable tableName="sample">
			<column name="id" type="varchar(36)">
				<constraints primaryKey="true" nullable="false" />
			</column>
			<column name="version" type="int" defaultValueNumeric="0" remarks="optimistic locking">
				<constraints nullable="false" />
			</column>
			<column name="insert_t" type="datetime" remarks="insert time">
				<constraints nullable="false" />
			</column>
			<column name="update_t" type="datetime" remarks="undate time">
				<constraints nullable="false" />
			</column>
		</createTable>
	
	</changeSet>
</databaseChangeLog>
